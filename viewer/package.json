{
  "name": "ngrx-visualizer",
  "version": "1.0.0",
  "description": "Visualizes an ngrx application",
  "main": "index.js",
  "scripts": {
    "build": "tsc && rollup -c",
    "serve": "live-server bin --watch=bin/**/*.html,bin/bundle.js,bin/**/*.css",
    "test": "jasmine-ts \"src/**/*.spec.ts\"",
    "tsc:w": "tsc -w",
    "rollup:w": "rollup -c -w",
    "start": "npm run build; npm-run-all --parallel serve tsc:w rollup:w"
  },
  "keywords": [],
  "author": "Google LLC",
  "license": "Apache-2.0",
  "dependencies": {
    "d3": "^5.5.0",
    "fuse-js-latest": "^3.1.0",
    "ts-node": "^7.0.1"
  },
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "@types/jasmine": "^2.8.8",
    "jasmine": "^3.2.0",
    "jasmine-ts": "^0.2.1",
    "karma": "^6.4.1",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-license": "^3.0.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-watch": "^4.3.1",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2"
  }
}
